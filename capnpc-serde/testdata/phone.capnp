@0xdbb9ad1f14bf0b36;  # unique file ID, generated by `capnp id`

struct Person {
  name @0 :Text;
  birthdate @3 :Date(Dymmy);

  email @1 :Text;
  phones @2 :List(PhoneNumber);

  struct PhoneNumber {
    number @0 :Text;
    type @1 :Type;

    enum Type {
      mobile @0;
      home @1;
      work @2;
    }
  }
}

struct Dymmy {
  year @0 :UInt16;
}

struct Date (YEARTYPE) {
  year @0 :YEARTYPE;
  month @1 :UInt8;
  day @2 :UInt8;
}